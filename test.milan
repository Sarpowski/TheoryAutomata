begin
  i := 0;

  while i < 10 do
    write(i);

    if i = 5 then
      i := 999
    else
      i := i + 1
    fi
  od;

  write(99);

  j := 0;
  while j < 10 do
    j := j + 1;

    if j = 3 then
      j := j
    else
      if j = 7 then
        j := 999
      else
        write(j)
      fi
    fi
  od
end